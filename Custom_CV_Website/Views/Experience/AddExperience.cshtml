@model EntityLayer.Concrete.Experience

@{
    ViewData["Title"] = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card-header border-0">
    <h3 class="mb-0">Deneyim Ekleme Sayfası</h3>
</div>

<br />
<br />

<form class="form-group" method="post">
    <label>Deneyim Başlığı</label>
    <input type="text" name="Name" class="form-control" />
    <br />
    <label>Tarih Bilgisi</label>
    <input type="text" name="Date" class="form-control" />
    <br />
    <label>Resim Yolu</label>
    <input type="text" name="ImageUrl" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" name="Description" class="form-control"/>
    <br />
    <button class="btn btn-outline-primary">Kaydet</button>
</form>

